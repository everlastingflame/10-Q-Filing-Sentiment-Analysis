,618\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n100.0\\n\\n%\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n6,254,072\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n100.0\\n\\n%\\n\\n\\n\\nCosts and expenses:\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\n\\nCost of revenue\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n1,085,748\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n61.9\\n\\n%\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n1,369,710\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n61.1\\n\\n%\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n3,210,998\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n61.3\\n\\n%\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n3,762,749\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n60.2\\n\\n%\\n\\n\\n\\nOperating, administrative and other\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n480,185\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n27.4\\n\\n%\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n530,919\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n23.7\\n\\n%\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n1,450,929\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n27.7\\n\\n%\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n1,572,233\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n25.1\\n\\n%\\n\\n\\n\\nDepreciation and amortization\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n90,494\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n5.1\\n\\n%\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n79,280\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n3.6\\n\\n%\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n250,591\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n4.8\\n\\n%\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n225,681\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n3.6\\n\\n%\\n\\n\\n\\nOperating income\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n97,738\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n\\u00a0\\n\\n5.6\\n\\n%\\n\\n\\u00a